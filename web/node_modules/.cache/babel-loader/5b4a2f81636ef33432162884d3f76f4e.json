ck</button> -->\r\n</template>\r\n<style lang=\"scss\" scoped>\r\n\r\n  .require{\r\n    color: red;\r\n  }\r\n\r\n  .danger input {\r\n    padding: .7rem;\r\n    font-size: 1rem;\r\n    border-radius: 5px;\r\n    border: 1px solid red;\r\n    color: #626262;\r\n    width: 100%;\r\n  }\r\n   .error{\r\n    color:red;\r\n     font-size:80%;\r\n  }\r\n</style>\r\n\r\n\r\n<script>\r\nimport Datepicker from 'vuejs-datepicker';\r\nimport service from '@/service/service';\r\n\r\nimport moment from 'moment';\r\n// import firebase from 'firebase/app'ว\r\n// import * as firebase from 'firebase/app';\r\n\r\n// import '@firebase/messaging';\r\nexport default {\r\n  components:{\r\n    Datepicker\r\n  },\r\n   data(){\r\n      return {\r\n        view_file:false,\r\n         'tableList': [\r\n        'vs-th: Component',\r\n        'vs-tr: Component',\r\n        'vs-td: Component',\r\n        'thread: Slot',\r\n        'tbody: Slot',\r\n        'header: Slot',\r\n      ],\r\n      popupActive:false,\r\n      renderComponent:true,\r\n      users: [\r\n\r\n      ],\r\n      status:[\r\n\r\n      ],\r\n      files:[],\r\n      user:{}\r\n\r\n      }\r\n   },\r\n\r\n   computed:{\r\n      validateForm() {\r\n            return !this.errors.any() && this.user.username != ''&&this.user.first_name!='' && this.user.last_name != '' && this.user.password != '' && this.user.confirm_password != '';\r\n        }\r\n   },\r\n   created(){\r\n      service.getData(\"/get_status\").then((result)=>{\r\n        this.status=result;\r\n      })\r\n      this.get_user();\r\n\r\n\r\n   },\r\n   methods: {\r\n     get_user(){\r\n       service.getData('/get_user').then((result)=>{\r\n        this.users=result.map((item)=>{\r\n          item.date_of_birth=item.date_of_birth==\"\"?\"\":moment(item.date_of_birth).format(\"DD/MM/YYYY\");\r\n          return item;\r\n        });\r\n      })\r\n     },\r\n      delete_user(user){\r\n        this.$swal({\r\n          title: 'Are you sure to delete this user?',\r\n          text: \"You won't be able to revert this!\",\r\n          type: 'warning',\r\n          showCancelButton: true,\r\n          confirmButtonColor: '#3085d6',\r\n          cancelButtonColor: '#d33',\r\n          confirmButtonText: 'delete'\r\n        }).then((result) => {\r\n          console.log(result);\r\n          if (result.value) {\r\n           service.postData(\"delete_user\",{username:user.username}).then((result)=>{\r\n\r\n              this.$swal(\r\n              'successfull!',\r\n              'user has been deleted.',\r\n              'success'\r\n            ).then((result)=>{\r\n              this.get_user();\r\n            })\r\n           })\r\n          }\r\n      })\r\n      },\r\n      edit(user){\r\n        this.user=user;\r\n        this.user.password=\"\";\r\n        this.user.password=\"\";\r\n        this.popupActive=true\r\n      },\r\n      save(){\r\n        if(this.validateForm){\r\n          service.postData(\"edit_user\",this.user).then((result)=>{\r\n            this.popupActive=false;\r\n            this.$swal(\r\n              'successfull!',\r\n              'user has been updated.',\r\n              'success'\r\n            ).then((result)=>{\r\n              this.get_user();\r\n            })\r\n          })\r\n        }\r\n      },\r\n      approve(user){\r\n        this.$swal({\r\n          title: 'Are you sure to approvice this user?',\r\n          text: \"You won't be able to revert this!\",\r\n          type: 'warning',\r\n          showCancelButton: true,\r\n          confirmButtonColor: '#3085d6',\r\n          cancelButtonColor: '#d33',\r\n          confirmButtonText: 'apprivce'\r\n        }).then((result) => {\r\n          console.log(result);\r\n          if (result.value) {\r\n           service.postData(\"approve\",{username:user.username}).then((result)=>{\r\n              this.$swal(\r\n              'successfull!',\r\n              'user has been approved.',\r\n              'success'\r\n            ).thenI((result)=>{\r\n              this.get_user();\r\n            })\r\n           })\r\n          }\r\n      })\r\n    },\r\n    download(file){\r\n      var win = window.open(\"http://localhost:3000/files/\"+file.file_name, '_blank');\r\n      win.focus();\r\n    },\r\n    delete_file(file){\r\n      this.$swal({\r\n          title: 'Are you sure to delete this file?',\r\n          text: \"You won't be able to revert this!\",\r\n          type: 'warning',\r\n          showCancelButton: true,\r\n          confirmButtonColor: '#3085d6',\r\n          cancelButtonColor: '#d33',\r\n          confirmButtonText: 'delete'\r\n        }).then((result) => {\r\n          console.log(result);\r\n          if (result.value) {\r\n            service.postData(\"delete_file\",{file_id:file.file_id}).then((result)=>{\r\n                this.$swal('file was deleted','','success').then((result)=>{\r\n                  this.forceRerender();\r\n                });\r\n              })\r\n          }\r\n      })\r\n\r\n    },\r\n    view(user){\r\n      console.log(user);\r\n     if(user.file_name!=\"\"&&user.file_name!=null){\r\n        var win = window.open(\"http://localhost:3000/files/\"+user.file_name, '_blank');\r\n        win.focus();\r\n     }else{\r\n       this.$swal('file does not exits','','warning')\r\n     }\r\n      // this.view_file=true;\r\n      // service.postData('get_file',{username:user.username}).then((result)=>{\r\n      //   this.files=result;\r\n      // })\r\n    }\r\n   },\r\n  watch: {\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n"],"sourceRoot":"src/views/demo"}]}